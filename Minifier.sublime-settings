{
    // the closure compiler adds new lines every 500 characters
    // for some proxies that choke with lines longer than that... don't care?
    // set this setting to true.
    "remove_new_lines": false,

    // the default optimization level to use for minification
    "optimization_level": "SIMPLE_OPTIMIZATIONS",

    // the compiler to use for minification.
    // Accepted values are: google_closure (don't use it anymore, it's broken and will be removed soon !)
    "compiler": "google_closure",

    // the compiler to use for minification.
    // Accepted values are: cssminifier
    "css_compiler": "cssminifier",

    // when you create a file you want to automatically open it?
    "open_on_min": false,

    // HTTP Timeout
    "timeout": 10,

    // Custom File Save
    "minify_options" : {
        "ask_file_name" : true,
        "default_name" : "file.js",
        "suffix" : ".min"
    }
}